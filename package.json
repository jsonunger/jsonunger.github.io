{
  "name": "website2",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jsonunger.com",
  "dependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/recompose": "^0.30.2",
    "@types/styled-components": "^4.1.19",
    "bootstrap": ">=4.1.2",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2",
    "recompose": "^0.30.0",
    "styled-components": "^4.1.3",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -b master -d build",
    "format": "prettier --write \"**/*.{js,ts,tsx,jsx,scss,css,json,prettierrc,babelrc}\"",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,scss,css,json,prettierrc,babelrc}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
